{
    "name": "cursor-rules-deploy",
    "version": "1.1.2",
    "description": "ðŸš€ A powerful CLI tool for deploying Cursor AI rules and templates - Enhance your development workflow with AI-powered coding assistance",
    "type": "module",
    "bin": {
        "cursor-rules-deploy": "./dist/bin/cli.js"
    },
    "scripts": {
        "build": "tsc && tsc-alias",
        "dev": "NODE_OPTIONS='--experimental-specifier-resolution=node --import tsx --import tsconfig-paths/register' tsx bin/cli.ts",
        "start": "node dist/bin/cli.js",
        "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --import tsx --import tsconfig-paths/register' tsx --test test/**/*.test.ts",
        "lint": "eslint ./src/**/*.ts",
        "format": "prettier --write .",
        "prepublishOnly": "npm run build && npm run test && npm run lint",
        "clean": "rimraf dist"
    },
    "keywords": [
        "cursor",
        "ai",
        "rules",
        "templates",
        "cli",
        "cursor-ai",
        "development-tools",
        "productivity",
        "code-generation",
        "ai-assistant"
    ],
    "author": {
        "name": "rosendolu",
        "url": "https://github.com/rosendolu"
    },
    "license": "MIT",
    "engines": {
        "node": ">=22.14.0"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.4.1",
        "@octokit/rest": "^21.1.1",
        "@types/globby": "^8.0.0",
        "chalk": "^5.3.0",
        "commander": "^11.0.0",
        "degit": "^2.8.4",
        "fs-extra": "^11.2.0",
