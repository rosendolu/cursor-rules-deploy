        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.15",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.7.1",
        "typescript": "^5.4.2",
        "typescript-eslint": "^8.29.1"
    },
